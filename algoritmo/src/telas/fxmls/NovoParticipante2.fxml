<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="486.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ControladorTelas">
   <children>
      <ImageView fitHeight="486.0" fitWidth="575.0">
         <image>
            <Image url="@../../imagens/novoParticipante.png" />
         </image>
      </ImageView>
      <Button fx:id="adicionarLigacao" blendMode="DARKEN" layoutX="76.0" layoutY="381.0" mnemonicParsing="false" onAction="#AddLigacao" prefHeight="38.0" prefWidth="170.0" text="Adicionar ligação" textFill="#e64343">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label layoutX="11.0" layoutY="35.0" text="O participante adicionado tem contato direto " textAlignment="CENTER" textFill="#e64343">
         <font>
            <Font name="Arial Narrow Bold Italic" size="16.0" />
         </font>
      </Label>
      <Button fx:id="sair" blendMode="DARKEN" layoutX="260.0" layoutY="381.0" mnemonicParsing="false" onAction="#Sair" prefHeight="38.0" prefWidth="94.0" text="Sair" textFill="#e64343">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <TableView fx:id="tabelapart" layoutX="76.0" layoutY="113.0" prefHeight="250.0" prefWidth="170.0">
         <columns>
            <TableColumn fx:id="participantes" prefWidth="169.0" text="Participantes" />
         </columns>
      </TableView>
      <Label layoutX="50.0" layoutY="51.0" text="com algum outro participante?" textAlignment="CENTER" textFill="#e64343">
         <font>
            <Font name="Arial Narrow Bold Italic" size="16.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="68.0" text="Se sim, selecione a pessoa e clique em adicionar" textAlignment="CENTER" textFill="#e64343">
         <font>
            <Font name="Arial Narrow Bold Italic" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
